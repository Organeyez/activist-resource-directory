<h1>User Show page</h1>

<%= link_to "My Collections", user_collections_path(current_user) %>
<%= link_to "My Favorites", user_favorites_path(current_user) %>



<h3>resources I have created</h3>
<% @resources.each do |resource|%>
	<div class="list-of-resources">
		<li><%= link_to resource.title, resource_path(resource) %></li>
		<li><%= resource.description %></li>	
	</div>
<% end  %>




